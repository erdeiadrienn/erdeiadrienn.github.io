
{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Tip percentage by day",
  "data": {
    "url": "tips.csv"
  },
  "transform": [
    {"calculate": "datum.tip / datum.total_bill", "as": "tip_percentage"}
  ],
  "mark": "bar",
  "encoding": {
    "x": {"field": "day", "type": "nominal"},
    "y": {"field": "tip_percentage", "type": "quantitative"}
  }
}
