
{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Tip Percentage by Party Size",
  "data": {
    "url": "tips.csv"
  },
  "transform": [
    {"calculate": "datum.tip / datum.total_bill", "as": "tip_percentage"}
  ],
  "mark": "bar",
  "encoding": {
    "x": {"field": "size", "type": "ordinal"},
    "y": {"field": "tip_percentage", "type": "quantitative"}
  }
}
